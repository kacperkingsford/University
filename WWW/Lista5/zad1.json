{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "accnumb": {
            "type": "string",
            "pattern": "[0-9]{26}"
        },
        "pesel": {
            "type": "string",
            "pattern": "[0-9]{11}"
        },
        "birthday": {
            "type": "string",
          "pattern": "([0-2][0-9]|(3)[0-1])(\/|.)(((0)[0-9])|((1)[0-2]))(\/|.)[0-9]{4}"  		
      },
        "email": {
            "type": "string",
            "format": "email"
        }
    },
    "required": [ "accnumb", "pesel", "birthday", "email"]
}